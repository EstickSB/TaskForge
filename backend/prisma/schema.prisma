generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model palabras{
  id Int @id @default(autoincrement())
  palabra String @unique
  letras Int 
  creacion DateTime @default(now())
}

model words{
  id Int @id @default(autoincrement())
  word String @unique
  letters Int 
  creation DateTime @default(now())
}